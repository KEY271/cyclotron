FoamFile
{
    version     2.3;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    phi
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-08;
        relTol          0.2;
    }

    rho
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-08;
        relTol          0.2;
    }
}

