FoamFile
{
    version     2.3;
    format      ascii;
    class       volScalarField;
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    dee
    {
        type            fixedValue;
        value           uniform 0;
    }

    filament
    {
        type            fixedValue;
        value           uniform 0;
    }

    body
    {
        type            fixedValue;
        value           uniform 0;
    }

    cable
    {
        type            fixedValue;
        value           uniform 0;
    }

    faraday
    {
        type            fixedValue;
        value           uniform 1;
    }
}

