FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    location  "system";
    object    meshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFile "constant/cyclotron2.stl";

maxCellSize 0.0005;

localRefinement
{
    "filament.*"
    {
        cellSize 0.0001;
    }
}

renameBoundary
{
    newPatchNames
    {
        "dummy.*"
        {
            newName "body";
            newType patch;
        }
        "body.*"
        {
            newName "body";
            newType patch;
        }
        "dee.*"
        {
            newName "dee";
            newType patch;
        }
        "filament.*"
        {
            newName "filament";
            newType patch;
        }
    }
}

